<div class="container-fluid">
  <div class="thumbnail">
   <img src="banner.png"></img>
  </div>
      <div class="row">
        <% @products.each do |product| %>
        <div class="col-xs-12 col-sm-4 col-md-8-offset-4">
            <%= link_to(image_tag(product.avatar_url(:large)), product_path(product), class: "thumbnail") %>
            <h4><%= product.title %></h4>
        </div>
        <% end %>
      </div>

</div>
